<div class="container">
  <div class="row justify-content-center">
    <div class="col-12 col-md-6">
      <h1>What chore have you done?</h1>
      <%= simple_form_for([@dinosaur, @completion]) do |f| %>
        <%= f.association :chore, as: :check_boxes %>
        <%= f.submit %>
      <% end %>
    </div>
  </div>
</div>
